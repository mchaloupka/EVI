<#Nuts0> a rr:TriplesMap;
	rr:logicalTable [ rr:tableName "[dbo].[Nuts0]"; ];
	rr:subjectMap [
		rr:template "http://ec.europa.eu/eurostat/ramon/rdfdata/nuts2008/{Code}";
		rr:class ec:NUTSRegion
	];
	rr:predicateObjectMap [
		rr:predicate ec:level;
		rr:objectMap [ rr:constant 0; ];
	];
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [ 
			rr:template "{Code} - {Name}";
			rr:termType rr:Literal;
		];
	];
	rr:predicateObjectMap [
		rr:predicate ec:name;
		rr:objectMap [ rr:column "[Name]"; ];
	];
	rr:predicateObjectMap [
		rr:predicate ec:regionCode;
		rr:objectMap [ rr:column "[Code]"; ];
	];
	rr:predicateObjectMap [
		rr:predicate ec:hasSubRegion;
		rr:objectMap [
			rr:parentTriplesMap <#Nuts1>;
			rr:joinCondition [
				rr:child "[Code]";
				rr:parent "[ParentRegion]";	
			];
		];
	];
.

